{% extends 'base.html.twig' %}

{% block title %}Ajouter une publication{% endblock %}

{% block body %}
<div class="d-flex justify-content-end position-fixed top-0 end-0 m-3">
    <a href="{{ app.request.headers.get('referer') }}">
        <img class="icon-page-precedente" src="{{ asset('assets/images/page_precedente.png') }}" alt="Icône non trouvé."></img>
    </a>
</div>

{{ form_start(form) }}

<div style="" class="p-2">

    {{ form_row( form.titre ) }}

    {{ form_row( form.contenu ) }}

    {{ form_row( form.legende ) }}

    {{ form_row( form.photo ) }}

    {{ form_row( form.submit ) }}

    {{ form_row(form.annuler) }}

</div>

{{ form_end(form) }}

{% endblock %}